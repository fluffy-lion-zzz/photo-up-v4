dAsDataURL(image)\n        reader.onloadend = (e) => {\n            setImagePreview(reader.result)\n        }\n        \n        setImageFile(imageFile => (image))\n        setShowImageInfo(true)\n        \n    }\n\n    return (\n        <Router>\n        <div className=\"uploaderWrapper\">\n        { showLocation === true ? \n            <div className=\"imageLocation\">\n                <ImageLocation\n                    storageRef={storageRef}\n                    currentFolder={currentFolder}\n                    setCurrentFolder={setCurrentFolder}\n                    setShowLocation={setShowLocation}\n                    setShowImage={setShowImage}\n                />\n            </div> \n            \n            \n            :\n            <></>\n        }\n        {\n            showImage === true ?\n        \n            <div className=\"selectImage\">\n                <div>\n                    <h3>being saved in...</h3>\n                    <h2>{currentFolder}</h2>\n                </div>\n                <div>\n                    <button onClick={() => setShowLocation(true)}>select a new folder</button>\n                </div>\n                {!showImageInfo ? \n                <div>\n                    <ImagePop \n                    handleImageFile={handleImageFile} \n                    imageFile={imageFile} \n                    setImageFile={setImageFile} \n                    reset={reset}\n                    setShowImageInfo={setShowImageInfo}\n                    />\n                </div>\n                :\n                <div>\n                    <ChooseImage \n                        imageFile={imageFile}\n                        setImageFile={setImageFile}\n                        setImageName={setImageName}\n                        imageName={imageName}\n                        handleUpload={handleUpload}\n                        handleImageFile={handleImageFile}\n                        imagePreview={imagePreview} \n                        loading={loading}\n                        storageRef={storageRef}\n                        currentFolder={currentFolder}\n                        setShowImageInfo={setShowImageInfo}\n                    />\n                </div>\n                \n                }\n            </div>  \n            :\n            <></>\n        }\n\n        \n        { showAddMeta ?\n\n        <div>\n            {uploadComplete ?\n                <div className=\"addMeta\">\n                    <UpdateMeta metaUploadRef={metaUploadRef}\n                    setReset={setReset}\n                    setImagePreview={setImagePreview}\n                    reset={reset}\n                    reseter={reseter}\n                    imagePreview={imagePreview}\n                    setShowAddMeta={setShowAddMeta}\n                    setShowImage={setShowImage}\n                    />\n                </div>\n                :\n                <Loading />\n            }\n\n        </div>\n            :\n            <></>\n        }\n        </div>\n        \n        </Router>  \n    )\n}\n\nexport default Uploader","/Users/codenation/Desktop/apprenticeshipProjects/photoUploader/photoUpV4/photo-uploader-v4/src/components/MetaSearch/MetaSearch.js",["186","187","188","189","190"],"import React, { useState, useEffect } from 'react'\nimport './MetaSearch.css'\nimport 'firebase/storage'\n\n\nconst MetaSearch = ({ storage }) => {\n\n    const [input, setInput] = useState(\"\")\n    // const [folders, setFolders] = useState([])\n    const [itemRef, setItemRef] = useState([])\n    const [urls , setUrls] = useState([])\n\n    const loadFiles = () => {\n        storage.ref().list().then(arr => {\n            arr.prefixes.map((folder) => {\n                folder.list().then((res) => {\n                    res.items.map((item) => {\n                        setItemRef(itemRef => [...itemRef, item.fullPath])\n                    })\n                })\n            })\n        })\n    }\n\n    useEffect(() => {\n        loadFiles()\n    },[])\n\n    const handleSearch = (e) => {\n        e.preventDefault()\n        setUrls([])\n        console.log(\"hit\")\n        itemRef.map((item) => {\n            storage.ref(item).getMetadata()\n            .then((metadata) => {\n                console.log(metadata)\n                let custom = metadata.customMetadata\n                    if(custom === undefined){\n                        console.log(\"no custom\")\n                    }else if (\n                    input === custom.tag1 ||\n                    input === custom.tag2 ||\n                    input === custom.tag3 ||\n                    input === custom.tag4 ||\n                    input === custom.tag5 ){\n                        let url = metadata.fullPath\n                        console.log(\"match : \", url)\n                        storage.ref(url).getDownloadURL().then((path) => {\n                            setUrls(urls => [...urls, path])\n                        })\n                    } else {\n                        console.log(\"no match\")\n                    }\n            })\n            .catch((error) =>{\n                console.log(error)\n            })\n        })\n\n        setInput(\"\")\n    }\n    return (\n        <div className=\"metaWrapper\">\n            <h1>tag search</h1>\n            <form onSubmit={handleSearch}>\n                <input \n                type=\"text\"\n                value={input}\n                onChange={(e) => setInput(e.target.value)}\n                />\n                <button type=\"submit\">search</button>\n            </form>\n            <div className=\"searchResCont\">\n                \n                {urls.map((url) => {\n                    return (\n                        <div className=\"imageCont\">\n                            <img src={url} />\n                        </div>\n                    )\n                })}\n            </div>\n        </div>\n    )\n}\n\nexport default MetaSearch\n","/Users/codenation/Desktop/apprenticeshipProjects/photoUploader/photoUpV4/photo-uploader-v4/src/components/Uploader/ChooseImage.js",["191","192","193"],"import React, { useState } from 'react'\nimport 'firebase/storage'\nimport './ChooseImage.css'\nimport ImageInfo from './ImageInfo'\nimport ImagePop from './ImagePop'\nimport Loading from '../Loading/Loading'\n// import ImageInfo from './ImageInfo'\n\n\nconst ChooseImage = ({ \n    imageFile, \n    setImageFile, \n    setImageName,\n    imageName, \n    handleUpload,\n    // handleImageFile,\n    storageRef,\n    imagePreview,\n    loading,\n    currentFolder,\n    setShowImageInfo\n    }) => {\n    \n    const isInvalid = imageFile === \"\" || imageFile === null\n\n    return (\n        <div className=\"chooseImg\">\n            <div>\n                <p>select a differnt image?</p>\n                <button onClick={()=> setShowImageInfo(false)}>change image</button>\n            </div>\n            <form onSubmit={handleUpload}>\n                <div>\n                    { loading ?\n                    <Loading /> :\n                    <img id=\"imagePreview\" src={imagePreview} />\n                    }\n                </div>\n                {/* <ImageInfo imageFile={imageFile}/> */}\n                <ImageInfo \n                    imageFile={imageFile}\n                    setImageFile={setImageFile}\n                    setImageName={setImageName}\n                    imageName={imageName}\n                    storageRef={storageRef}\n                    currentFolder={currentFolder}\n                />\n                <div className=\"uploadCont\">\n                    <button disabled={isInvalid} type=\"submit\">upload</button>\n                </div>\n            </form>\n        </div>\n    )\n}\n\nexport default ChooseImage","/Users/codenation/Desktop/apprenticeshipProjects/photoUploader/photoUpV4/photo-uploader-v4/src/components/Uploader/ImageInfo.js",["194","195"],"import React, { useState, useEffect } from 'react'\nimport 'firebase/storage'\n// import AddMeta from '../AddMeta/AddMeta'\nimport './ImageInfo.css'\nconst ImageInfo = ({ imageFile, imageName, setImageName, storageRef, currentFolder}) => {\n    const [imageNames, setImageNames] = useState([])\n    const [nameInput, setNameInput] = useState(\"\")\n    const [display, setDisplay] = useState(\"\")\n\n    const isInvalid = imageNames.includes(nameInput) || nameInput === \"\"\n    const handleName = (e) => {\n        e.preventDefault()\n        setDisplay(nameInput)\n        setImageName(nameInput)\n    }\n    const collect = () => {\n        storageRef.child(`${currentFolder}`)\n        .list()\n        .then(res => {\n            res.items.map((item) => {\n                setImageNames(imageNames => [...imageNames, item.name])\n            })\n        })\n    }\n\n    useEffect(()=> {\n        setImageNames([])\n        collect()\n    },[currentFolder])\n\n    return (\n        <div className=\"imageInfo\">\n            <h1>image info</h1>\n            {imageFile ? \n            <p>original name : {imageFile.name}</p> :\n            <></>\n            }\n            {imageNames.includes(imageFile.name) ?\n            <div>\n                <p>the original file name is taken</p>\n                <p>if you dont the original file will be overwritten...just sayin</p>\n            </div>\n            :\n            <></>\n            }\n            <p>do you want to enter a new name?</p>\n            {/* <p>if you dont the original file will be overwritten...just sayin</p> */}\n            <input \n                type=\"text\"\n                value={nameInput}\n                onChange={(e) => {setNameInput(e.target.value)}}\n            />\n            <button disabled={isInvalid} onClick={handleName}>submit new name</button>\n            {display !== \"\" ?\n            <div>\n                <p>new file name: {display}</p>\n            </div>\n            :\n            <div></div>\n            }\n        </div>\n    )\n}\n\nexport default ImageInfo",["196","197"],"/Users/codenation/Desktop/apprenticeshipProjects/photoUploader/photoUpV4/photo-uploader-v4/src/components/Uploader/ImageLocation.js",["198","199","200","201"],"import React, { useState, useEffect } from 'react'\nimport \"firebase/storage\"\nimport './ImageLocation.css'\n\n\nconst SelectFolder = ({ folders, setCurrentFolder, currentFolder, setShowLocation }) => {\nconst [newFolder, setNewFolder] = useState(\"\")\n const isInvalid = \n        newFolder === \"\" ||\n        newFolder === \"--\" ||\n        currentFolder === \"--\" ||\n        currentFolder === \"\"\n        \n    const newFolderHandler = (e) => {\n        e.preventDefault()\n        console.log(folders)\n        if(folders.includes(newFolder)){\n            console.log(\"folder already exists\")\n        } else {\n            setCurrentFolder(newFolder)\n            \n        }        \n    }\n    \n\n    return (\n        <div>\n            <select\n                value={currentFolder}\n                onChange={(e) => setCurrentFolder(e.target.value)}\n            >\n                <option>--</option>\n                {folders.map((folderNames)=> {\n                    return <option value={folderNames}>{folderNames}</option>\n                    })\n                }\n            </select>\n            <h2>new folder</h2>\n            <form onSubmit={newFolderHandler}>\n                <input\n                    value={newFolder}\n                    onChange={(e) => setNewFolder(e.target.value)}\n                />\n                <button type=\"submit\" disabled={isInvalid}>\n                add folder\n                </button>\n                {currentFolder === \"\" || currentFolder ===\"--\" ?\n                    <p>you have to select a folder yo</p> :\n                    <></>}\n            </form>\n        </div>\n    )\n}\nconst ImageLocation = ({ storageRef, currentFolder, setCurrentFolder, setS